{
  "name": "sensrnet-registery-backend",
  "version": "0.0.5",
  "description": "SensRNet Registery Backend Application",
  "author": "https://github.com/kadaster-labs/sensrnet-home/blob/master/MAINTAINERS.md",
  "license": "EUPL-1.2",
  "repository": {
    "url": "https://github.com/kadaster-labs/sensrnet-backend"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "uml": "tplant -C --input src/events/**/*.ts --output docs/images/events-uml.svg"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.13",
    "@nestjs/swagger": "^4.5.7",
    "@nestjs/core": "^7.0.13",
    "@nestjs/cqrs": "^7.0.0",
    "@nestjs/mongoose": "^7.0.1",
    "@nestjs/platform-express": "^7.0.13",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/jwt": "^7.0.0",
    "passport-jwt": "^4.0.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "geteventstore-promise": "^3.2.1",
    "moment": "^2.26.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "swagger-ui-express": "^4.1.4",
    "mongoose": "^5.9.15",
    "@nestjs/websockets": "^7.1.0",
    "@nestjs/platform-socket.io": "^7.1.0",
    "cookie-parser": "^1.4.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "25.2.3",
    "@types/mongoose": "5.7.21",
    "@types/node": "^13.9.1",
    "@types/passport-local": "1.0.33",
    "@types/socket.io": "^2.1.8",
    "@types/supertest": "2.0.8",
    "jest": "26.0.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "tplant": "^2.3.3",
    "ts-jest": "26.1.0",
    "ts-node": "^8.6.2",
    "tsc-watch": "4.0.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
