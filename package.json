{
  "name": "sensrnet-registery-backend",
  "version": "0.9.0",
  "description": "SensRNet Registery Backend Application",
  "author": "https://github.com/kadaster-labs/sensrnet-home/blob/main/MAINTAINERS.md",
  "license": "EUPL-1.2",
  "repository": {
    "url": "https://github.com/kadaster-labs/sensrnet-backend"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --forceExit",
    "uml": "tplant -C --input src/core/events/**/*.ts --output docs/images/events-uml.svg"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/cqrs": "^8.0.1",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^9.0.2",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/platform-socket.io": "^8.2.6",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/terminus": "^8.0.4",
    "@nestjs/websockets": "^8.2.6",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cookie-parser": "^1.4.5",
    "geteventstore-promise": "^4.0.0",
    "helmet": "^4.6.0",
    "jwks-rsa": "^1.12.3",
    "migrate-mongo": "^8.1.4",
    "moment": "^2.26.0",
    "mongoose": "^6.2.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.2.6",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.0",
    "@types/node": "^17.0.14",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.4.7",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "tplant": "^2.3.3",
    "ts-jest": "^27.1.3",
    "ts-node": "^8.6.2",
    "tsc-watch": "4.0.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.0",
    "typescript": "^4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
